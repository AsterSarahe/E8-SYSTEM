def query_user_fetchone():
try
dbconfig = read_db_config()
conn = MySQLConnection(**dbconfig)
cursor = conn.cursor()
cursor.execute("SELECT * FROM user")
row = cursor.fetchone()
connection.close()

while row is not None:
print(row)
row = cursor.fetchone()

except Error as e:
print(e)
        
finally:
cursor.close()
conn.close()

